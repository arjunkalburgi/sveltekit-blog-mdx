{
  "name": "sveltekit-blog-mdx",
  "version": "0.2.2",
  "scripts": {
    "dev": "svelte-kit dev --port 3030",
    "build": "npm run generate:manifest && svelte-kit build",
    "preview": "svelte-kit preview --port 3030",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:manifest": "node ./generate-manifest.js",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fontsource/lato": "^4.5.0",
    "@fontsource/lora": "^4.5.0",
    "@fontsource/source-sans-pro": "^4.5.0",
    "@imgix/js-core": "^3.2.1",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@sveltejs/adapter-netlify": "^1.0.0-next.19",
    "@sveltejs/kit": "^1.0.0-next.122",
    "dayjs": "^1.10.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": "^7.0.1",
    "mdsvex": "^0.9.8",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rehype-slug": "^4.0.1",
    "sass": "^1.35.2",
    "sharp": "^0.28.3",
    "svelte": "^3.38.3",
    "svelte-feather-icons": "^3.5.0",
    "svelte-preprocess": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-seo"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-seo/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run prettier:check"
    }
  },
  "type": "module"
}
